            const originalWindow = global.window;
            delete global.window;
            const preferences = (0, _themeUtils.detectSystemPreferences)();
            expect(preferences).toEqual({
                colorScheme: "light",
                reducedMotion: false,
                highContrast: false,
                forcedColors: false
            });
            global.window = originalWindow;
        });

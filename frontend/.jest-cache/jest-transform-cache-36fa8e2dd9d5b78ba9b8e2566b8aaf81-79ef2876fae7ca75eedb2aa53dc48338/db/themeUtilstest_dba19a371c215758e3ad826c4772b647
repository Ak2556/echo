            // Restore geolocation
            Object.defineProperty(navigator, "geolocation", {
                writable: true,
                value: originalGeolocation
            });
        });
        it("handles reverse geocoding API with ok response but failed parse", async ()=>{
            const mockPosition = {
                coords: {

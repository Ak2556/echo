      cov_ksjmo7ihv().b[8][0]++;
      cov_ksjmo7ihv().s[20]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[8][1]++;
    }
    const newOtp =
    /* istanbul ignore next */
    (cov_ksjmo7ihv().s[21]++, [...otp]);
    // Handle multiple digits (paste)
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[22]++;
    if (value.length > 1) {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[9][0]++;
      const digits =
      /* istanbul ignore next */
      (cov_ksjmo7ihv().s[23]++, value.slice(0, length - index).split(''));
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[24]++;
      digits.forEach((digit, i) => {
        /* istanbul ignore next */
        cov_ksjmo7ihv().f[6]++;
        cov_ksjmo7ihv().s[25]++;
        if (index + i < length) {
          /* istanbul ignore next */
          cov_ksjmo7ihv().b[10][0]++;
          cov_ksjmo7ihv().s[26]++;
          newOtp[index + i] = digit;
        } else
        /* istanbul ignore next */
        {
          cov_ksjmo7ihv().b[10][1]++;
        }
      });
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[27]++;
      setOtp(newOtp);
      // Focus last filled or next empty
      const nextIndex =
      /* istanbul ignore next */
      (cov_ksjmo7ihv().s[28]++, Math.min(index + digits.length, length - 1));
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[29]++;
      inputRefs.current[nextIndex]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[30]++;
      setActiveIndex(nextIndex);
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[31]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[9][1]++;
    }
    // Single digit
    cov_ksjmo7ihv().s[32]++;
    newOtp[index] = value;
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[33]++;
    setOtp(newOtp);
    // Move to next input
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[34]++;
    if (
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[12][0]++, value) &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[12][1]++, index < length - 1)) {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[11][0]++;
      cov_ksjmo7ihv().s[35]++;
      inputRefs.current[index + 1]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[36]++;
      setActiveIndex(index + 1);
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[11][1]++;
    }
  };
  /* istanbul ignore next */
  cov_ksjmo7ihv().s[37]++;
  const handleKeyDown = (e, index) => {
    /* istanbul ignore next */
    cov_ksjmo7ihv().f[7]++;
    cov_ksjmo7ihv().s[38]++;
    // Backspace
    if (e.key === 'Backspace') {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[13][0]++;
      cov_ksjmo7ihv().s[39]++;
      if (
      /* istanbul ignore next */
      (cov_ksjmo7ihv().b[15][0]++, !otp[index]) &&
      /* istanbul ignore next */
      (cov_ksjmo7ihv().b[15][1]++, index > 0)) {
        /* istanbul ignore next */
        cov_ksjmo7ihv().b[14][0]++;
        // Move to previous input and clear it
        const newOtp =
        /* istanbul ignore next */
        (cov_ksjmo7ihv().s[40]++, [...otp]);
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[41]++;
        newOtp[index - 1] = '';
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[42]++;
        setOtp(newOtp);
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[43]++;
        inputRefs.current[index - 1]?.focus();
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[44]++;
        setActiveIndex(index - 1);
      } else {
        /* istanbul ignore next */
        cov_ksjmo7ihv().b[14][1]++;
        // Clear current input
        const newOtp =
        /* istanbul ignore next */
        (cov_ksjmo7ihv().s[45]++, [...otp]);
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[46]++;
        newOtp[index] = '';
        /* istanbul ignore next */
        cov_ksjmo7ihv().s[47]++;
        setOtp(newOtp);
      }
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[48]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[13][1]++;
    }
    // Left arrow
    cov_ksjmo7ihv().s[49]++;
    if (
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[17][0]++, e.key === 'ArrowLeft') &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[17][1]++, index > 0)) {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[16][0]++;
      cov_ksjmo7ihv().s[50]++;
      inputRefs.current[index - 1]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[51]++;
      setActiveIndex(index - 1);
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[52]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[16][1]++;
    }
    // Right arrow
    cov_ksjmo7ihv().s[53]++;
    if (
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[19][0]++, e.key === 'ArrowRight') &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[19][1]++, index < length - 1)) {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[18][0]++;
      cov_ksjmo7ihv().s[54]++;
      inputRefs.current[index + 1]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[55]++;
      setActiveIndex(index + 1);
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[56]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[18][1]++;
    }
    // Home
    cov_ksjmo7ihv().s[57]++;
    if (e.key === 'Home') {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[20][0]++;
      cov_ksjmo7ihv().s[58]++;
      inputRefs.current[0]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[59]++;
      setActiveIndex(0);
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[60]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[20][1]++;
    }
    // End
    cov_ksjmo7ihv().s[61]++;
    if (e.key === 'End') {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[21][0]++;
      cov_ksjmo7ihv().s[62]++;
      inputRefs.current[length - 1]?.focus();
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[63]++;
      setActiveIndex(length - 1);
      /* istanbul ignore next */
      cov_ksjmo7ihv().s[64]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[21][1]++;
    }
  };
  /* istanbul ignore next */
  cov_ksjmo7ihv().s[65]++;
  const handlePaste = e => {
    /* istanbul ignore next */
    cov_ksjmo7ihv().f[8]++;
    cov_ksjmo7ihv().s[66]++;
    e.preventDefault();
    const pastedData =
    /* istanbul ignore next */
    (cov_ksjmo7ihv().s[67]++, e.clipboardData.getData('text/plain').trim());
    // Only paste if all digits
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[68]++;
    if (!/^\d+$/.test(pastedData)) {
      /* istanbul ignore next */
      cov_ksjmo7ihv().b[22][0]++;
      cov_ksjmo7ihv().s[69]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_ksjmo7ihv().b[22][1]++;
    }
    const digits =
    /* istanbul ignore next */
    (cov_ksjmo7ihv().s[70]++, pastedData.slice(0, length).split(''));
    const newOtp =
    /* istanbul ignore next */
    (cov_ksjmo7ihv().s[71]++, new Array(length).fill(''));
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[72]++;
    digits.forEach((digit, i) => {
      /* istanbul ignore next */
      cov_ksjmo7ihv().f[9]++;
      cov_ksjmo7ihv().s[73]++;
      newOtp[i] = digit;
    });
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[74]++;
    setOtp(newOtp);
    // Focus last input
    const lastIndex =
    /* istanbul ignore next */
    (cov_ksjmo7ihv().s[75]++, Math.min(digits.length - 1, length - 1));
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[76]++;
    inputRefs.current[lastIndex]?.focus();
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[77]++;
    setActiveIndex(lastIndex);
  };
  /* istanbul ignore next */
  cov_ksjmo7ihv().s[78]++;
  const handleFocus = index => {
    /* istanbul ignore next */
    cov_ksjmo7ihv().f[10]++;
    cov_ksjmo7ihv().s[79]++;
    setActiveIndex(index);
    // Select text on focus
    /* istanbul ignore next */
    cov_ksjmo7ihv().s[80]++;
    inputRefs.current[index]?.select();
  };
  /* istanbul ignore next */
  cov_ksjmo7ihv().s[81]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: "jsx-dde2b16255136282" + " " + "otp-input-container",
    children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "jsx-dde2b16255136282" + " " + "flex gap-3 justify-center mb-4",
      children: otp.map((digit, index) => {
        /* istanbul ignore next */
        cov_ksjmo7ihv().f[11]++;
        cov_ksjmo7ihv().s[82]++;
        return /*#__PURE__*/(0, _jsxruntime.jsx)(_framermotion.motion.div, {
          initial: {
            opacity: 0,
            y: -20
          },
          animate: {
            opacity: 1,
            y: 0
          },
          transition: {
            delay: index * 0.05
          },
          children: /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
            ref: el => {
              /* istanbul ignore next */
              cov_ksjmo7ihv().f[12]++;
              cov_ksjmo7ihv().s[83]++;
              inputRefs.current[index] = el;
            },
            type: "text",
            inputMode: "numeric",
            maxLength: 1,
            value: digit,
            onChange: e => {
              /* istanbul ignore next */
              cov_ksjmo7ihv().f[13]++;
              cov_ksjmo7ihv().s[84]++;
              return handleChange(e.target.value, index);
            },
            onKeyDown: e => {
              /* istanbul ignore next */
              cov_ksjmo7ihv().f[14]++;
              cov_ksjmo7ihv().s[85]++;
              return handleKeyDown(e, index);
            },
            onPaste: handlePaste,
            onFocus: () => {
              /* istanbul ignore next */
              cov_ksjmo7ihv().f[15]++;
              cov_ksjmo7ihv().s[86]++;
              return handleFocus(index);
            },
            disabled: loading,
            "aria-label": `Digit ${index + 1} of ${length}`,
            "aria-invalid": !!error,
            className: "jsx-dde2b16255136282" + " " + `
                w-12 h-14 text-center text-2xl font-bold
                border-2 rounded-xl
                transition-all duration-200
                focus:outline-none focus:ring-2 focus:ring-offset-2
                ${error ?
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[23][0]++, 'border-red-500 focus:ring-red-500 bg-red-50') :
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[23][1]++, activeIndex === index ?
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[24][0]++, 'border-indigo-600 focus:ring-indigo-600 bg-white') :
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[24][1]++, digit ?
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[25][0]++, 'border-green-500 bg-green-50') :
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[25][1]++, 'border-gray-300 bg-gray-50')))}
                ${loading ?
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[26][0]++, 'opacity-50 cursor-not-allowed') :
            /* istanbul ignore next */
            (cov_ksjmo7ihv().b[26][1]++, 'cursor-text')}
                hover:border-indigo-400
              `
          })
        }, index);
      })
    }),
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[27][0]++, error) &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[27][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_framermotion.motion.p, {
      initial: {
        opacity: 0,
        y: -10
      },
      animate: {
        opacity: 1,
        y: 0
      },
      className: "text-red-500 text-sm text-center mb-2",
      role: "alert",
      children: error
    })),
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[28][0]++, loading) &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[28][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)(_framermotion.motion.div, {
      initial: {
        opacity: 0
      },
      animate: {
        opacity: 1
      },
      className: "flex items-center justify-center gap-2 text-gray-600",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "jsx-dde2b16255136282" + " " + "w-4 h-4 border-2 border-indigo-600 border-t-transparent rounded-full animate-spin"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
        className: "jsx-dde2b16255136282",
        children: "Verifying..."
      })]
    })),
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[29][0]++, !loading) &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[29][1]++, !error) &&
    /* istanbul ignore next */
    (cov_ksjmo7ihv().b[29][2]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("p", {
      className: "jsx-dde2b16255136282" + " " + "text-gray-500 text-sm text-center",
      children: ["Enter the ", length, "-digit code sent to your email"]
    })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      role: "status",
      "aria-live": "polite",
      "aria-atomic": "true",
      className: "jsx-dde2b16255136282" + " " + "sr-only",
      children: [otp.filter(d => {
        /* istanbul ignore next */
        cov_ksjmo7ihv().f[16]++;
        cov_ksjmo7ihv().s[87]++;
        return d;
      }).length, " of ", length, " digits entered",
      /* istanbul ignore next */
      (cov_ksjmo7ihv().b[30][0]++, error) &&
      /* istanbul ignore next */
      (cov_ksjmo7ihv().b[30][1]++, `. Error: ${error}`)]
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_style.default, {
      id: "dde2b16255136282",
      children: '@media(prefers-reduced-motion:reduce){input.jsx-dde2b16255136282{transition:none!important}}input.jsx-dde2b16255136282::-webkit-outer-spin-button,input.jsx-dde2b16255136282::-webkit-inner-spin-button{-webkit-appearance:none;margin:0}input[type="number"].jsx-dde2b16255136282{-moz-appearance:textfield}'
    })]
  });
}

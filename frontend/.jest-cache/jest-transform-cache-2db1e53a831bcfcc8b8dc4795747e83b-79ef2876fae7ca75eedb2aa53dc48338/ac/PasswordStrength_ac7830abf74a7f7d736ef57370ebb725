/**
 * Calculate password strength score and suggestions.
 */
function calculateStrength(password, strengthLabels) {
  /* istanbul ignore next */
  cov_lt2wjnohp().f[1]++;
  cov_lt2wjnohp().s[6]++;
  if (
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[1][0]++, !password) ||
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[1][1]++, !password.trim())) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[0][0]++;
    cov_lt2wjnohp().s[7]++;
    return {
      score: 0,
      strength: 'weak',
      color: '#EF4444',
      label: 'Very Weak',
      suggestions: ['Enter a password to check strength']
    };
  } else
  /* istanbul ignore next */
  {
    cov_lt2wjnohp().b[0][1]++;
  }
  let score =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[8]++, 0);
  const suggestions =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[9]++, []);
  // Simple scoring: 20 points per criteria (100 total)
  // Length check (20 points)
  /* istanbul ignore next */
  cov_lt2wjnohp().s[10]++;
  if (password.length >= 8) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[2][0]++;
    cov_lt2wjnohp().s[11]++;
    score += 20;
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[2][1]++;
    cov_lt2wjnohp().s[12]++;
    suggestions.push('Use at least 8 characters');
  }
  // Uppercase letters (20 points)
  /* istanbul ignore next */
  cov_lt2wjnohp().s[13]++;
  if (/[A-Z]/.test(password)) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[3][0]++;
    cov_lt2wjnohp().s[14]++;
    score += 20;
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[3][1]++;
    cov_lt2wjnohp().s[15]++;
    suggestions.push('Add uppercase letters');
  }
  // Lowercase letters (20 points)
  /* istanbul ignore next */
  cov_lt2wjnohp().s[16]++;
  if (/[a-z]/.test(password)) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[4][0]++;
    cov_lt2wjnohp().s[17]++;
    score += 20;
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[4][1]++;
    cov_lt2wjnohp().s[18]++;
    suggestions.push('Add lowercase letters');
  }
  // Numbers (20 points)
  /* istanbul ignore next */
  cov_lt2wjnohp().s[19]++;
  if (/[0-9]/.test(password)) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[5][0]++;
    cov_lt2wjnohp().s[20]++;
    score += 20;
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[5][1]++;
    cov_lt2wjnohp().s[21]++;
    suggestions.push('Add numbers');
  }
  // Special characters (20 points)
  /* istanbul ignore next */
  cov_lt2wjnohp().s[22]++;
  if (/[^A-Za-z0-9]/.test(password)) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[6][0]++;
    cov_lt2wjnohp().s[23]++;
    score += 20;
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[6][1]++;
    cov_lt2wjnohp().s[24]++;
    suggestions.push('Add special characters (!@#$%^&*)');
  }
  // Additional checks for variety and patterns
  const uniqueChars =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[25]++, new Set(password.split('')));
  /* istanbul ignore next */
  cov_lt2wjnohp().s[26]++;
  if (
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[8][0]++, password.length > 0) &&
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[8][1]++, uniqueChars.size / password.length < 0.6)) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[7][0]++;
    cov_lt2wjnohp().s[27]++;
    suggestions.push('Avoid repeated characters');
  } else
  /* istanbul ignore next */
  {
    cov_lt2wjnohp().b[7][1]++;
  }
  // Common patterns penalty
  const commonPatterns =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[28]++, ['123', 'abc', 'qwerty', 'password', 'admin']);
  const lowerPassword =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[29]++, password.toLowerCase());
  /* istanbul ignore next */
  cov_lt2wjnohp().s[30]++;
  if (commonPatterns.some(pattern => {
    /* istanbul ignore next */
    cov_lt2wjnohp().f[2]++;
    cov_lt2wjnohp().s[31]++;
    return lowerPassword.includes(pattern);
  })) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[9][0]++;
    cov_lt2wjnohp().s[32]++;
    suggestions.push('Avoid common patterns');
  } else
  /* istanbul ignore next */
  {
    cov_lt2wjnohp().b[9][1]++;
  }
  // Determine strength level (to match test expectations)
  let strength;
  let color;
  let label;
  /* istanbul ignore next */
  cov_lt2wjnohp().s[33]++;
  if (score <= 20) {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[10][0]++;
    cov_lt2wjnohp().s[34]++;
    strength = 'weak';
    /* istanbul ignore next */
    cov_lt2wjnohp().s[35]++;
    color = '#EF4444';
    /* istanbul ignore next */
    cov_lt2wjnohp().s[36]++;
    label =
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[11][0]++, strengthLabels?.[20]) ||
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[11][1]++, 'Weak');
  } else {
    /* istanbul ignore next */
    cov_lt2wjnohp().b[10][1]++;
    cov_lt2wjnohp().s[37]++;
    if (score <= 60) {
      /* istanbul ignore next */
      cov_lt2wjnohp().b[12][0]++;
      cov_lt2wjnohp().s[38]++;
      strength = 'fair';
      /* istanbul ignore next */
      cov_lt2wjnohp().s[39]++;
      color = '#F59E0B';
      /* istanbul ignore next */
      cov_lt2wjnohp().s[40]++;
      label =
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[13][0]++, strengthLabels?.[60]) ||
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[13][1]++, 'Fair');
    } else {
      /* istanbul ignore next */
      cov_lt2wjnohp().b[12][1]++;
      cov_lt2wjnohp().s[41]++;
      if (score <= 80) {
        /* istanbul ignore next */
        cov_lt2wjnohp().b[14][0]++;
        cov_lt2wjnohp().s[42]++;
        strength = 'good';
        /* istanbul ignore next */
        cov_lt2wjnohp().s[43]++;
        color = '#10B981';
        /* istanbul ignore next */
        cov_lt2wjnohp().s[44]++;
        label =
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[15][0]++, strengthLabels?.[80]) ||
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[15][1]++, 'Good');
      } else {
        /* istanbul ignore next */
        cov_lt2wjnohp().b[14][1]++;
        cov_lt2wjnohp().s[45]++;
        strength = 'strong';
        /* istanbul ignore next */
        cov_lt2wjnohp().s[46]++;
        color = '#059669';
        /* istanbul ignore next */
        cov_lt2wjnohp().s[47]++;
        label =
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[16][0]++, strengthLabels?.[100]) ||
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[16][1]++, 'Strong');
      }
    }
  }
  /* istanbul ignore next */
  cov_lt2wjnohp().s[48]++;
  return {
    score,
    strength,
    color,
    label,
    suggestions
  };
}
function PasswordStrength({
  password,
  className =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[17][0]++, ''),
  showAdvanced =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[18][0]++, false),
  minLength =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[19][0]++, 8),
  hideCriteria =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[20][0]++, []),
  hideLength =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[21][0]++, false),
  strengthLabels,
  customLabels =
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[22][0]++, {})
}) {
  /* istanbul ignore next */
  cov_lt2wjnohp().f[3]++;
  // Handle null/undefined password
  const safePassword =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[49]++,
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[23][0]++, password) ||
  /* istanbul ignore next */
  (cov_lt2wjnohp().b[23][1]++, ''));
  const result =
  /* istanbul ignore next */
  (cov_lt2wjnohp().s[50]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_lt2wjnohp().f[4]++;
    cov_lt2wjnohp().s[51]++;
    return calculateStrength(safePassword, strengthLabels);
  }, [safePassword, strengthLabels]));
  /* istanbul ignore next */
  cov_lt2wjnohp().s[52]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: `space-y-2 ${className}`,
    children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "space-y-1",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex items-center justify-between text-sm",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          className: "text-gray-600 dark:text-gray-400",
          children: "Password Strength"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          className: `font-medium ${result.label === 'Very Weak' ?
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[24][0]++, 'text-red-600') :
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[24][1]++, result.strength === 'weak' ?
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[25][0]++, 'text-red-500') :
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[25][1]++, result.strength === 'fair' ?
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[26][0]++, 'text-yellow-500') :
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[26][1]++, result.strength === 'good' ?
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[27][0]++, 'text-blue-500') :
          /* istanbul ignore next */
          (cov_lt2wjnohp().b[27][1]++, 'text-green-500'))))}`,
          style: {
            color: result.color
          },
          "aria-live": "polite",
          children: result.label
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "h-2 bg-gray-200 dark:bg-gray-700 rounded-full overflow-hidden",
        role: "progressbar",
        "aria-valuemin": 0,
        "aria-valuemax": 100,
        "aria-valuenow": result.score,
        "aria-label": "Password strength",
        style: {
          width: `${result.score}%`
        },
        children: /*#__PURE__*/(0, _jsxruntime.jsx)(_framermotion.motion.div, {
          className: "h-full rounded-full",
          initial: {
            width: 0
          },
          animate: {
            width: `${result.score}%`
          },
          transition: {
            duration: 0.3
          },
          style: {
            backgroundColor: result.color,
            width: `${result.score}%`
          }
        })
      })]
    }),
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[28][0]++, result.suggestions.length > 0) &&
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[28][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)(_framermotion.motion.ul, {
      className: "text-xs text-gray-600 dark:text-gray-400 space-y-1 pl-4",
      initial: {
        opacity: 0,
        y: -10
      },
      animate: {
        opacity: 1,
        y: 0
      },
      transition: {
        duration: 0.2
      },
      children: result.suggestions.map((suggestion, index) => {
        /* istanbul ignore next */
        cov_lt2wjnohp().f[5]++;
        cov_lt2wjnohp().s[53]++;
        return /*#__PURE__*/(0, _jsxruntime.jsx)(_framermotion.motion.li, {
          className: "list-disc",
          initial: {
            opacity: 0,
            x: -10
          },
          animate: {
            opacity: 1,
            x: 0
          },
          transition: {
            delay: index * 0.05
          },
          children: suggestion
        }, index);
      })
    })),
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[29][0]++, showAdvanced) &&
    /* istanbul ignore next */
    (cov_lt2wjnohp().b[29][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "text-xs space-y-1",
      children: [
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[30][0]++, safePassword) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[30][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "text-green-600 dark:text-green-400",
        children: "Good character diversity"
      })),
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[31][0]++, safePassword.toLowerCase().includes('password')) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[31][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "text-red-500 dark:text-red-400",
        children: "Avoid dictionary words"
      })),
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[32][0]++, !['password', '123456', 'qwerty'].some(common => {
        /* istanbul ignore next */
        cov_lt2wjnohp().f[6]++;
        cov_lt2wjnohp().s[54]++;
        return safePassword.toLowerCase().includes(common);
      })) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[32][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "text-green-600 dark:text-green-400",
        children: "Not a common password"
      }))]
    })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "grid grid-cols-2 gap-2 text-xs",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(RequirementItem, {
        met: safePassword.length >= minLength,
        label: `${minLength}+ characters`
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(RequirementItem, {
        met: /[A-Z]/.test(safePassword),
        label: "Uppercase"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(RequirementItem, {
        met: /[a-z]/.test(safePassword),
        label: "Lowercase"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(RequirementItem, {
        met: /[0-9]/.test(safePassword),
        label: "Number"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)(RequirementItem, {
        met: /[^A-Za-z0-9]/.test(safePassword),
        label: "Special character"
      })]
    }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "text-xs space-y-1",
      children: [
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[33][0]++, !hideLength) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[33][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: `${safePassword.length >= minLength ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[34][0]++, 'text-green-500') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[34][1]++, 'text-red-500')}`,
        children: ["At least ", minLength, " characters"]
      })), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `${/[A-Z]/.test(safePassword) ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[35][0]++, 'text-green-500') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[35][1]++, 'text-red-500')}`,
        children: "Contains uppercase letter"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `${/[a-z]/.test(safePassword) ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[36][0]++, 'text-green-500') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[36][1]++, 'text-red-500')}`,
        children: "Contains lowercase letter"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `${/[0-9]/.test(safePassword) ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[37][0]++, 'text-green-500') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[37][1]++, 'text-red-500')}`,
        children: "Contains number"
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `${/[^A-Za-z0-9]/.test(safePassword) ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[38][0]++, 'text-green-500') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[38][1]++, 'text-red-500')}`,
        children: "Contains special character"
      }),
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[39][0]++, !showAdvanced) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[39][1]++, result.label !== 'Strong') &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[39][2]++, /*#__PURE__*/(0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          children: "Strong"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          children: "Good character diversity"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          children: "Avoid dictionary words"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          children: "Not a common password"
        })]
      })),
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[40][0]++, showAdvanced) &&
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[40][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `${['password123', '123456789', 'qwerty123'].some(common => {
          /* istanbul ignore next */
          cov_lt2wjnohp().f[7]++;
          cov_lt2wjnohp().s[55]++;
          return safePassword.toLowerCase() === common;
        }) ?
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[41][0]++, 'text-red-500 dark:text-red-400') :
        /* istanbul ignore next */
        (cov_lt2wjnohp().b[41][1]++, 'text-green-600 dark:text-green-400')}`,

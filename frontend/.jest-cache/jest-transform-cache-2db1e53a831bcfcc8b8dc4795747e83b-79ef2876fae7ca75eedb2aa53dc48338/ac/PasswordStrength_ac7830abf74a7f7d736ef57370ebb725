  });
}
function RequirementItem({
  met,
  label
}) {
  /* istanbul ignore next */
  cov_lt2wjnohp().f[9]++;
  cov_lt2wjnohp().s[57]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)(_framermotion.motion.div, {
    className: "flex items-center gap-1.5",
    initial: {
      opacity: 0
    },
    animate: {
      opacity: 1
    },
    transition: {
      duration: 0.2
    },
    children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: `w-4 h-4 rounded-full flex items-center justify-center text-white text-[10px] ${met ?
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[43][0]++, 'bg-green-500') :
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[43][1]++, 'bg-red-500')}`,
      children: met ?
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[44][0]++, '') :
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[44][1]++, '')
    }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
      className: met ?
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[45][0]++, 'text-green-600 dark:text-green-400') :
      /* istanbul ignore next */
      (cov_lt2wjnohp().b[45][1]++, 'text-red-500 dark:text-red-400'),
      children: label
    })]
  });
}

            /*#__PURE__*/ (0, _jsxruntime.jsx)(_EditRecordModal.default, {
                visible: showModal === 'editRecord',
                record: selectedRecord,
                language: language,
                onClose: ()=>{
                    setShowModal(null);
                    setSelectedRecord(null);
                },
                onSave: (updates)=>{
                    if (!selectedRecord) return;
                    updateCattleRecord(selectedRecord.id, updates);
                    setShowModal(null);
                    setSelectedRecord(null);
                }

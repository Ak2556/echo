            it('detects number sequences', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "12345678"
                }));
                expect(_react1.screen.getByText(/Avoid common patterns/i)).toBeInTheDocument();
            });
            it('detects alphabet sequences', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "abcdefgh"
                }));
                expect(_react1.screen.getByText(/Avoid common patterns/i)).toBeInTheDocument();
            });
            it('detects repeated characters', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "aaaaaaaa"
                }));
                expect(_react1.screen.getByText(/Avoid repeated characters/i)).toBeInTheDocument();
            });
            it('detects password variations', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "password123"
                }));
                expect(_react1.screen.getByText(/Avoid common patterns/i)).toBeInTheDocument();
            });
            it('detects admin variations', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "admin1234"
                }));
                expect(_react1.screen.getByText(/Avoid common patterns/i)).toBeInTheDocument();
            });
        });
        describe('Component Lifecycle', ()=>{
            it('handles rapid mount/unmount', ()=>{
                for(let i = 0; i < 20; i++){
                    const { unmount } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                        password: "test"
                    }));
                    unmount();
                }
                // Should not leak memory
                expect(true).toBe(true);
            });
            it('updates when password prop changes', ()=>{
                const { rerender } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "weak"
                }));
                rerender(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "StrongPassword123!"
                }));
                expect(_react1.screen.getByText('Strong')).toBeInTheDocument();
            });
            it('handles null password gracefully', ()=>{
                const { container } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: null
                }));
                expect(container).toBeInTheDocument();
                expect(_react1.screen.getByText('Very Weak')).toBeInTheDocument();
            });
            it('handles undefined password gracefully', ()=>{
                const { container } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: undefined
                }));
                expect(container).toBeInTheDocument();
                expect(_react1.screen.getByText('Very Weak')).toBeInTheDocument();
            });
        });
        describe('Edge Case Password Patterns', ()=>{
            it('handles all special characters', ()=>{
                const specialChars = '!@#$%^&*()_+-=[]{}|;:,.<>?/~`';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: specialChars
                }));
                expect(_react1.screen.getByRole('progressbar')).toBeInTheDocument();
            });
            it('handles mixed case with numbers and symbols', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "Aa1!Bb2@Cc3#"
                }));
                expect(_react1.screen.getByText('Strong')).toBeInTheDocument();
            });
            it('handles password with newlines', ()=>{
                const passwordWithNewline = 'Pass\nword123!';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: passwordWithNewline
                }));
                expect(_react1.screen.getByRole('progressbar')).toBeInTheDocument();
            });
            it('handles password with tabs', ()=>{
                const passwordWithTab = 'Pass\tword123!';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: passwordWithTab
                }));
                expect(_react1.screen.getByRole('progressbar')).toBeInTheDocument();
            });
            it('handles Base64 encoded strings', ()=>{
                const base64 = 'SGVsbG9Xb3JsZDEyMyE=';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: base64
                }));
                expect(_react1.screen.getByRole('progressbar')).toBeInTheDocument();
            });
        });
        describe('Accessibility Under Stress', ()=>{
            it('maintains ARIA attributes during rapid updates', ()=>{
                const { rerender } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "test1"
                }));
                for(let i = 0; i < 10; i++){
                    rerender(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                        password: `test${i}`
                    }));
                    const progressbar = _react1.screen.getByRole('progressbar');
                    expect(progressbar).toHaveAttribute('aria-valuemin', '0');
                    expect(progressbar).toHaveAttribute('aria-valuemax', '100');
                    expect(progressbar).toHaveAttribute('aria-valuenow');
                }
            });
            it('updates live region correctly', ()=>{
                const { rerender } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "weak"
                }));
                const liveRegion = _react1.screen.getByText('Weak');
                expect(liveRegion).toHaveAttribute('aria-live', 'polite');
                rerender(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "StrongPassword123!"
                }));
                expect(_react1.screen.getByText('Strong')).toHaveAttribute('aria-live', 'polite');
            });
        });
        describe('Prop Validation Edge Cases', ()=>{
            it('handles negative minLength', ()=>{
                const { container } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "test",
                    minLength: -5
                }));
                expect(container).toBeInTheDocument();
            });
            it('handles extremely large minLength', ()=>{
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "test",
                    minLength: 10000
                }));
                expect(_react1.screen.getByText('At least 10000 characters')).toBeInTheDocument();
            });
            it('handles showAdvanced toggle during render', ()=>{
                const { rerender } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "Password123!",
                    showAdvanced: false
                }));
                rerender(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "Password123!",
                    showAdvanced: true
                }));
                expect(_react1.screen.getByText(/character diversity/i)).toBeInTheDocument();
            });
            it('handles empty strengthLabels object', ()=>{
                const { container } = (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "Test123!",
                    strengthLabels: {}
                }));
                expect(container).toBeInTheDocument();
            });
            it('handles partial strengthLabels', ()=>{
                const partialLabels = {
                    100: 'Great'
                };
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: "StrongPass123!",
                    strengthLabels: partialLabels
                }));
                expect(_react1.screen.getByText('Great')).toBeInTheDocument();
            });
        });
        describe('Real-World Password Scenarios', ()=>{
            it('handles passphrase style passwords', ()=>{
                const passphrase = 'correct horse battery staple 123!';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: passphrase
                }));
                expect(_react1.screen.getByText('Strong')).toBeInTheDocument();
            });
            it('handles leetspeak passwords', ()=>{
                const leetspeak = 'P@55w0rd!23';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: leetspeak
                }));
                expect(_react1.screen.getByRole('progressbar')).toBeInTheDocument();
            });
            it('handles randomly generated passwords', ()=>{
                const random = 'xK9#mP2$qL5@wN8!';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: random
                }));
                expect(_react1.screen.getByText('Strong')).toBeInTheDocument();
            });
            it('handles common substitution patterns', ()=>{
                const substitution = 'Password123!';
                (0, _testutils.render)(/*#__PURE__*/ (0, _jsxruntime.jsx)(_PasswordStrength.default, {
                    password: substitution
                }));
                // Should detect password pattern
                expect(_react1.screen.getByText(/Avoid common patterns/i)).toBeInTheDocument();
            });
        });
    });
